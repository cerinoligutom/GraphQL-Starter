{
  "name": "graphql-starter",
  "description": "GraphQL Starter",
  "private": true,
  "scripts": {
    "release": "standard-version",
    "prepare": "husky install",
    "start": "npx gulp dev",
    "build": "npx gulp build",
    "build:prod": "npx gulp build --prod",
    "lint": "eslint --cache ./src",
    "prettier": "prettier --write \"src/**/*.{js,ts,json,css,scss,md}\"",
    "migrate:make": "knex --knexfile ./knexfile.* migrate:make -x ts",
    "migrate:latest": "knex --knexfile ./knexfile.* migrate:latest",
    "migrate:rollback": "knex --knexfile ./knexfile.* migrate:rollback",
    "seed:make": "knex --knexfile ./knexfile.* seed:make -x ts",
    "seed": "knex --knexfile ./knexfile.* seed:run",
    "test": "echo \"No test specified\" && exit 0",
    "tsc-check": "npx tsc --noEmit",
    "generate:gql-types": "npx graphql-codegen",
    "apollo:service:push": "npx apollo service:push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerino-ligutom/GraphQL-Starter.git"
  },
  "author": {
    "email": "zeferinix@gmail.com",
    "name": "Cerino O. Ligutom III",
    "url": "https://www.zeferinix.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cerino-ligutom/GraphQL-Starter/issues"
  },
  "homepage": "https://github.com/cerino-ligutom/GraphQL-Starter#readme",
  "dependencies": {
    "@graphql-tools/load-files": "6.3.2",
    "@graphql-tools/merge": "6.2.14",
    "apollo-server-express": "2.25.0",
    "bcryptjs": "2.4.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dataloader": "2.0.0",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-redis-subscriptions": "2.4.0",
    "graphql-scalars": "1.9.3",
    "graphql-upload": "^12.0.0",
    "helmet": "4.6.0",
    "ioredis": "4.27.5",
    "jsonwebtoken": "8.5.1",
    "knex": "0.95.4",
    "objection": "2.2.14",
    "objection-cursor": "1.2.4",
    "pg": "8.6.0",
    "supertokens-node": "5.0.1",
    "tsconfig-paths": "3.9.0",
    "uuid": "8.3.2",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@graphql-codegen/add": "2.0.2",
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-resolvers": "1.19.2",
    "@types/bcryptjs": "2.4.2",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/graphql-depth-limit": "1.1.2",
    "@types/graphql-iso-date": "3.4.0",
    "@types/graphql-upload": "8.0.4",
    "@types/gulp": "4.0.8",
    "@types/ioredis": "4.26.4",
    "@types/node": "15.12.1",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "@zeferinix/commitlint-config": "2.1.1",
    "copyfiles": "2.4.1",
    "eslint": "7.28.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "gulp": "4.0.2",
    "gulp-run-command": "0.0.10",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1",
    "rimraf": "3.0.2",
    "standard-version": "9.3.0",
    "ts-node": "10.0.0",
    "tsc-alias": "1.3.1",
    "typescript": "4.3.2"
  }
}
