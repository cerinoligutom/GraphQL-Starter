{
  "name": "graphql-starter",
  "description": "GraphQL Starter",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "release": "standard-version",
    "prepare": "husky install",
    "start": "npx gulp dev",
    "build": "npx gulp build",
    "build:prod": "npx gulp build --prod",
    "lint": "eslint --cache ./src",
    "prettier": "prettier --write \"src/**/*.{js,ts,json,css,scss,md}\"",
    "migrate:make": "knex --knexfile ./knexfile.* migrate:make -x ts",
    "migrate:latest": "knex --knexfile ./knexfile.* migrate:latest",
    "migrate:rollback": "knex --knexfile ./knexfile.* migrate:rollback",
    "seed:make": "knex --knexfile ./knexfile.* seed:make -x ts",
    "seed": "knex --knexfile ./knexfile.* seed:run",
    "test": "echo \"No test specified\" && exit 0",
    "tsc-check": "npx tsc --noEmit",
    "generate:gql-types": "npx graphql-codegen",
    "apollo:service:push": "npx apollo service:push",
    "generate": "npx hygen generator generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerino-ligutom/GraphQL-Starter.git"
  },
  "author": {
    "email": "zeferinix@gmail.com",
    "name": "Cerino O. Ligutom III",
    "url": "https://www.zeferinix.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cerino-ligutom/GraphQL-Starter/issues"
  },
  "homepage": "https://github.com/cerino-ligutom/GraphQL-Starter#readme",
  "dependencies": {
    "@graphql-tools/load-files": "6.6.1",
    "@graphql-tools/merge": "8.3.12",
    "@graphql-tools/schema": "9.0.10",
    "apollo-server-core": "3.11.1",
    "apollo-server-express": "3.11.1",
    "bcryptjs": "2.4.3",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dataloader": "2.1.0",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-redis-subscriptions": "^2.5.0",
    "graphql-scalars": "1.20.1",
    "graphql-subscriptions": "2.0.0",
    "graphql-ws": "5.11.2",
    "helmet": "6.0.0",
    "ioredis": "5.2.4",
    "jsonwebtoken": "8.5.1",
    "knex": "2.3.0",
    "objection": "3.0.1",
    "objection-cursor": "1.2.6",
    "patch-package": "6.5.0",
    "pg": "8.8.0",
    "subscriptions-transport-ws": "0.11.0",
    "supertokens-node": "11.1.1",
    "tsconfig-paths": "4.1.0",
    "uuid": "9.0.0",
    "ws": "8.11.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "17.2.0",
    "@graphql-codegen/add": "3.2.1",
    "@graphql-codegen/cli": "2.13.12",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.8.2",
    "@graphql-codegen/typescript-resolvers": "2.7.7",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.14",
    "@types/graphql-depth-limit": "1.1.3",
    "@types/gulp": "4.0.10",
    "@types/ioredis": "5.0.0",
    "@types/node": "18.11.9",
    "@types/uuid": "8.3.4",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "@zeferinix/commitlint-config": "2.1.1",
    "eslint": "8.28.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "gulp": "4.0.2",
    "gulp-run-command": "0.0.10",
    "husky": "8.0.2",
    "hygen": "6.2.11",
    "inquirer": "8.2.4",
    "lint-staged": "13.0.3",
    "node-dev": "7.4.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "standard-version": "9.5.0",
    "ts-node": "10.9.1",
    "tsc-alias": "1.7.1",
    "typescript": "4.8.4"
  }
}
